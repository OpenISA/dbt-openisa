    "version": "0.2.0",
    "configurations": [
        {
            "name": "(gdb) Launch",
            "type": "cppdbg",
            "request": "launch",
            "program": "${workspaceFolder}/build/oi-dbt",
            
            "args": ["-bin", "/home/napoli/Documents/OpenISA/oi-dbt/oi-dbt/test/bin/argv_param-oi", "-rft", "netplus", "-v", "-hot", "50", "--args", "argvposition"],
            "stopAtEntry": true,
            "cwd": "${workspaceFolder}",
            "externalConsole": false,
            "internalConsoleOptions": "openOnSessionStart",
            "environment": [],
            "MIMode": "gdb",
            "setupCommands": [
                {
                    "text": "-enable-pretty-printing", 
                    "ignoreFailures": true,
                    "externalConsole": false,
                    "description": "Enable pretty-printing for gdb",
                }
                   
            ]
        }
    ]
}